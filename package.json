    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/postgres": "^0.5.1",
    "algoliasearch": "^4.24.0",
    "autoprefixer": "^10.4.20",
    "axios": "^0.21.4",
    "btoa": "^1.2.1",
